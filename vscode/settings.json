{
	"editor.minimap.enabled": false,
	"workbench.editor.enablePreview": false,
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.scrollback": 1000000,
	"workbench.editorjsociations": {
		"0": {
			"viewType": "jupyter.notebook.ipynb",
			"filenamePattern": "*.ipynb"
		}
	},
	"git.confirmSync": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"window.menuBarVisibility": "compact",
	"editor.fontSize": 18,
	"editor.fontFamily": "'JetBrains Mono'",
	"extensions.autoCheckUpdates": false,
	"extensions.autoUpdate": "onlySelectedExtensions",
	"update.mode": "manual",
	"git.openRepositoryInParentFolders": "never",
	"go.toolsManagement.autoUpdate": true,
	"gitlens.codeLens.authors.enabled": false,
	"gitlens.showWhatsNewAfterUpgrades": false,
	"gitlens.codeLens.enabled": false,
	"gitlens.hovers.annotations.over": "annotation",
	"gitlens.hovers.annotations.changes": false,
	"gitlens.hovers.annotations.details": false,
	"gitlens.hovers.annotations.enabled": false,
	"gitlens.hovers.avatars": false,
	"gitlens.hovers.autolinks.enhanced": false,
	"gitlens.hovers.autolinks.enabled": false,
	"gitlens.hovers.currentLine.changes": false,
	"gitlens.hovers.currentLine.details": false,
	"gitlens.hovers.currentLine.enabled": false,
	"gitlens.hovers.enabled": false,
	"debug.onTaskErrors": "abort",
	"editor.lineNumbers": "relative",
	
	"vim.normalModeKeyBindings": [
		{
			"before": ["<C-j>"],
			"after": ["j", "z", "z"]
		},
		{
			"before": ["<S-j>"],
			"after": ["<C-e>"]
		},
		{
			"before": ["<C-k>"],
			"after": ["k", "z", "z"]
		},
		{
			"before": ["<S-k>"],
			"after": ["<C-y>"]
		},
	],
	
	"vim.handleKeys": {
		"<C-p>": false,
		"<C-b>": false,
		"<C-f>": false,
		"<C-w>": false
	},
	"workbench.colorTheme": "Night Owl Black (No Italics)",
	"explorer.confirmDragAndDrop": false,
	"workbench.startupEditor": "none"
}